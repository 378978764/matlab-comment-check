{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "es2019",
		"lib": ["ES2019"],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "out",
		"rootDir": "src",
		"skipLibCheck": true,
		"strict": true
	},
	"include": ["src"],
	"exclude": ["./node_modules", ".vscode-test"]
}
