{
  "name": "matlab_commment_checker",
  "description": "A variable comment checker for matlab.",
  "author": "shengdi",
  "license": "MIT",
  "version": "1.0.0",
  "icon": "./images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/378978764/matlab-comment-check.git"
  },
  "publisher": "vscode-samples",
  "categories": [],
  "keywords": [
    "matlab comment variables"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "onLanguage:matlab"
  ],
  "main": "./client/out/extension",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "sh ./scripts/e2e.sh"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "mocha": "^8.1.1",
    "@types/node": "^12.12.0",
    "eslint": "^6.4.0",
    "@typescript-eslint/parser": "^2.3.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "chardet": "^1.3.0"
  }
}
